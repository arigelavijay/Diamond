                            @if (HtmlHelpers.hasRight("ActivitySummary"))
                            {
                                <li>
                                    <a href="@Url.Action("Index", "Dashboard", new { area = "Dashboard" }, null)">
                                        <i class="fa fa-calculator"></i>Activity Summary
                                    </a>
                                </li>
                            }
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-asterisk" aria-hidden="true"></i><span>Master Data</span> <i class="fa fa-angle-left pull-right"></i>
