                            }
                            @if (HtmlHelpers.hasRight("ProductCategory"))
                            {
                                <li><a href="@Url.Action("ProductCategoryList", "MasterData", new { area = "MasterData" }, null)"><i class="fa fa-product-hunt" aria-hidden="true"></i>&nbsp;Product Category</a></li>
                            }
                            @if (HtmlHelpers.hasRight("Location"))
                            {
                                <li><a href="@Url.Action("LocationList", "MasterData", new { area = "MasterData" }, null)"><i class="fa fa-map-marker" aria-hidden="true"></i>&nbsp;Location</a></li>
                            }
                            @if (HtmlHelpers.hasRight("PaymentType"))
                            {
                                <li><a href="@Url.Action("PaymentTypeList", "MasterData", new { area = "MasterData" }, null)"><i class="fa fa-credit-card-alt" aria-hidden="true"></i>&nbsp;Payment Type</a></li>
                            }
                            @if (HtmlHelpers.hasRight("Products"))
                            {
                                <li><a href="@Url.Action("Products", "MasterData", new { area = "MasterData" }, null)"><i class="fa fa-briefcase" aria-hidden="true"></i>&nbsp;Products</a></li>
                            }
                            @if (HtmlHelpers.hasRight("Supplier"))
                            {
                                <li><a href="@Url.Action("Suppliers", "MasterData", new { area = "MasterData" }, null)">&nbsp;<i class="fa fa-male" aria-hidden="true"></i>&nbsp;Supplier</a></li>
                            }
                            @if (HtmlHelpers.hasRight("Customer"))
                            {
                                <li><a href="@Url.Action("Customers", "MasterData", new { area = "MasterData" }, null)"><i class="fa fa-users" aria-hidden="true"></i>&nbsp;Customer</a></li>
                            }
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-fighter-jet" aria-hidden="true"></i><span>Quotation</span> <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            @if (HtmlHelpers.hasRight("StandardQuotation"))
                            {
                                <li><a href="@Url.Action("StandardRateProfile", "Quotation", new { area = "Quotation", quotationNo = "STANDARD", isLink = true }, null)"><i class="fa fa-link"></i>&nbsp;Standard Quotation</a></li>
                            }
                            @if (HtmlHelpers.hasRight("CustomerTariff"))
