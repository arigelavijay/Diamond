                        <ul class="treeview-menu">
                            @if (HtmlHelpers.hasRight("Company"))
                            {
                                <li><a href="@Url.Action("CompanyProfile", "MasterData", new { area = "MasterData" }, null)"><i class="fa fa-building-o" aria-hidden="true"></i>&nbsp;Company</a></li>
                            }
                            @if (HtmlHelpers.hasRight("UOM"))
