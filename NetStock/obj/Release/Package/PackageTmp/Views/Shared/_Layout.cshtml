                        </a>
                        <ul class="treeview-menu">
                            @if (HtmlHelpers.hasRight("ShipmentIndentList"))
                            {
                                <li><a href="@Url.Action("ShipmentIndent", "Reports", new { area = "Reports" }, null)"><i class="fa fa-list-ol" aria-hidden="true"></i>&nbsp;Shipment Indent List</a></li>
                            }
