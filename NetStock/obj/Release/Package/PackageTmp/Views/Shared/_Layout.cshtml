    @Html.Partial("_ShowAlertPartial")
    @Html.Partial("_ShowConfirmDialog")

    <script src="@Url.Content("~/Scripts/jquery-1.10.2.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap.js")"></script>
    <script src="@Url.Content("~/Scripts/app.js")"></script>
    <script type="text/javascript">
        var RawUrl = '@Request.Url';
        var ErrorUrl = '@Url.Content("~/Account/Login")';
        var dateFormatStr = '';
        if (RawUrl.indexOf('http://localhost') == 0)
            dateFormatStr = 'MM/DD/YY';
        else
            dateFormatStr = 'MM/DD/YYYY';
    </script>
    <script src="@Url.Content("~/Scripts/js/layout.js")"></script>


    @RenderSection("scripts", required: false)
