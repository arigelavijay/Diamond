                        </a>
                        <ul class="treeview-menu">
                            @if (HtmlHelpers.hasRight("StockInquiry"))
                            {
                                <li><a href="@Url.Action("StockInquiry", "Operation", new { area = "Operation" }, null)"><i class="fa fa-stack-exchange" aria-hidden="true"></i>&nbsp;Stock Inquiry</a></li>
                            }
                            @if (HtmlHelpers.hasRight("InvoiceInquiry"))
                            {
                                <li><a href="@Url.Action("InvoiceInquiry", "Billing", new { area = "Billing" }, null)">&nbsp;<i class="fa fa-info" aria-hidden="true"></i>&nbsp;Invoice Inquiry</a></li>
                            }
